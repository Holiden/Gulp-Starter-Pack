{{#shapes}}
@mixin {{#selector.dimensions}}{{expression}}{{/selector.dimensions}}($position, $size) {
  background-image: url("./../images/sprites/sprite.svg#{{#selector.dimensions}}{{expression}}{{/selector.dimensions}}");
  background-position: $position;
  background-size: $size;
  background-repeat: no-repeat;
}

{{/shapes}}
