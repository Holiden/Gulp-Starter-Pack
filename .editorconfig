root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = space
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.min.*]
insert_final_newline = ignore
indent_style = ignore
trim_trailing_whitespace = false

[*.svg]
insert_final_newline = false
